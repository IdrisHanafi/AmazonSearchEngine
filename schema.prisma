datasource db {
  // postgresql DB
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator db {
  provider  = "prisma-client-py"
  interface = "asyncio"
}

model Products {
  id            Int     @id @default(autoincrement())
  asin          Int     @unique
  title         String
  description   String[]
  imageURL      String[]
  category      String[]
  price         String?
  brand         String?
}
